@page
@model ProEShop.Web.Pages.SellerPanel.Consignment.ConfirmationConsignmentModel
@{
    ViewData["Title"] = "محموله ایجاد شد";
}

<div class="container">
    <div class="row justify-content-center align-items-center">
        <div class="card text-center col-lg-8 col-xl-6 py-3">
            <div class="card-header">
                فروشگاه
                <a asp-page="/Index">
                    پرو شاپ
                    <img src="~/images/application/eshop.png" width="130" />
                </a>
            </div>
            <div class="card-body">
                <div class="alert alert-success text-center">
                    محموله شما با موفقیت ایجاد شد، پس از تایید انباردار شما میتوانید محموله خود را به سمت
                    انبار های پرو شاپ ارسال نمایید.
                    <br />
                    <span class="text-primary">
                        مراتب از طریق ایمیل به شما اطلاع رسانی خواهد شد.
                    </span>
                </div>
            </div>
        </div>
    </div>
</div>